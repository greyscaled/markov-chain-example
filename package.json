{
  "name": "markov-chain-example",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://vapurrmaid.ca/markov-chain-example",
  "dependencies": {
    "@vapurrmaid/markov-chain": "^0.4.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typestyle": "^2.1.0"
  },
  "scripts": {
    "build": "react-scripts build",
    "dev": "react-scripts start",
    "format": "prettier --check '**/*.{css,html,js,json,md,ts,tsx,yaml,yml}'",
    "format:write": "prettier --write '**/*.{css,html,js,json,md,ts,tsx,yaml,yml}'",
    "lint": "eslint . --cache --color --ext ts,tsx --max-warnings 0",
    "lint:fix": "eslint . --color --ext ts,tsx --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.8",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vapurrmaid/prettier-config": "^0.3.1",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.2.1",
    "prettier-plugin-organize-imports": "^2.1.0",
    "react-scripts": "^4.0.1",
    "typescript": "^4.3.4"
  },
  "lint-staged": {
    "*.{css,html,json,md,yaml,yml}": "prettier --write",
    "*.{ts,tsx}": [
      "eslint --fix --max-warnings=0",
      "prettier --write"
    ]
  }
}
